;
; BIND reverse data file for local loopback interface
;
$TTL 1h	; Default TTL
@	IN	SOA	mail.larendacs.pc.	admin.mail.larendacs.pc. (
	2019022001	; serial
	1h		; slave refresh interval
	15m		; slave retry interval
	1w		; slave copy expire time
	1h		; NXDOMAIN cache time
	)

;
; domain name servers
;
@	IN	NS	mail.larendacs.pc.

; IPv6 PTR entries
8.7.0    IN    PTR    mail2.larendacs.pc.

